  "components": {
    "schemas": {
      "AdminUpdateUserDTO": {
        "required": [
          "fullName",
          "isLocked",
          "roleId"
        ],
        "type": "object",
        "properties": {
          "fullName": {
            "maxLength": 255,
            "minLength": 1,
            "type": "string"
          },
          "phone": {
            "type": "string",
            "format": "tel",
            "nullable": true
          },
          "roleId": {
            "type": "integer",
            "format": "int32"
          },
          "isLocked": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "CategoryCreateDto": {
        "required": [
          "categoryName"
        ],
        "type": "object",
        "properties": {
          "categoryName": {
            "maxLength": 50,
            "minLength": 3,
            "pattern": "^[\\w\\s\\-]+$",
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "CategoryDto": {
        "type": "object",
        "properties": {
          "categoryId": {
            "type": "integer",
            "format": "int32"
          },
          "categoryName": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CategoryUpdateDto": {
        "required": [
          "categoryId",
          "categoryName"
        ],
        "type": "object",
        "properties": {
          "categoryId": {
            "type": "integer",
            "format": "int32"
          },
          "categoryName": {
            "maxLength": 50,
            "minLength": 3,
            "pattern": "^[\\w\\s\\-]+$",
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "ChangePasswordRequestDTO": {
        "required": [
          "confirmPassword",
          "newPassword",
          "oldPassword"
        ],
        "type": "object",
        "properties": {
          "oldPassword": {
            "minLength": 1,
            "type": "string"
          },
          "newPassword": {
            "minLength": 8,
            "pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[^A-Za-z0-9]).+$",
            "type": "string"
          },
          "confirmPassword": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "DocumentCreateDto": {
        "required": [
          "author",
          "categoryId",
          "condition",
          "title"
        ],
        "type": "object",
